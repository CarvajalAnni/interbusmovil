<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bordes"
    tools:context=".Perfil">

    <ImageButton
        android:id="@+id/Imagen1"
        android:layout_width="66dp"
        android:layout_height="79dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.147"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.064"
        app:srcCompat="@mipmap/flechaatras" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="84dp"
        android:layout_height="77dp"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.899"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logoazul" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="136dp"
        android:layout_height="131dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.61"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.215"
        app:srcCompat="@mipmap/conductor" />

    <TextView
        android:id="@+id/textusuario"
        android:layout_width="342dp"
        android:layout_height="51dp"
        android:layout_marginTop="304dp"
        android:gravity="center"
        android:hint="Usuario"
        android:textColorHint="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.768"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textNombreP"
        android:layout_width="154dp"
        android:layout_height="52dp"
        android:layout_marginTop="40dp"
        android:gravity="center"
        android:hint="Correo"
        android:textColorHint="@color/grisinter"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.583"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textusuario"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textCorreoP"
        android:layout_width="155dp"
        android:layout_height="58dp"
        android:layout_marginTop="88dp"
        android:gravity="center"
        android:hint="ContraseÃ±a"
        android:textSize="25sp"
        android:textColorHint="@color/grisinter"

        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.582"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textNombreP"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textRolP"
        android:layout_width="158dp"
        android:layout_height="61dp"
        android:layout_marginTop="92dp"
        android:gravity="center"
        android:hint="Rol"
        android:textSize="25sp"
        android:textColorHint="@color/grisinter"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.588"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textCorreoP"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>